<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.studyit.mvc.IBoard_Report_List_DAO">

	<select id="list" resultType="com.studyit.mvc.Board_Report_List_DTO">
		SELECT ROWNUM, ID, POST_CODE, TITLE, HANDLE_RESULT, HANDLE_DATE
		FROM VIEW_RH_BOARDLIST
		ORDER BY HANDLE_DATE DESC
	</select>
	
	<select id="count" resultType="java.lang.Integer">
		SELECT COUNT(*)
		FROM VIEW_RH_BOARDLIST
	</select>
</mapper>